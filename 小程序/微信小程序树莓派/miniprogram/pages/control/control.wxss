/* pages/control/control.wxss */
 
.control {
  width: 100%;
  height: 100%;
  transform: rotate(90deg);
  background-color: #eee;
  justify-content: center;
}
 
.cameraView {
  margin-left: 0px;
  width: 603px;
  height: 375px;
  background-color: #eee;
  justify-content: center;
}
 
.button {
  height: 60px;
  width: 60px;
  opacity: 0.3;
}
 
#forward {
  left: 60px;
  top: 135px;
}
 
#left {
  left: 0px;
  top: 195px;
}
 
#right {
  left: 120px;
  top: 195px;
}
 
#backLeft {
